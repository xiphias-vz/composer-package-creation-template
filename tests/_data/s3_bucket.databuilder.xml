<?xml version="1.0"?>
<transfers xmlns="spryker:databuilder-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd">

    <transfer name="BucketAndFileNames">
        <property name="bucketName" dataBuilderRule="realText(20, 2)"/>
        <property name="fileNames" dataBuilderRule="words(2, false)"/>
    </transfer>

    <transfer name="S3Upload">
        <property name="bucket" dataBuilderRule="word()"/>
        <property name="uploadedFile" dataBuilderRule="="/>
    </transfer>

</transfers>
